{
  "extends": "../../tsconfig.base.json",
  "files": [],
  "include": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.spec.json"
    },
    {
      "path": "./tsconfig.editor.json"
    }
  ],
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@atocha/core/data-access-firebase": ["../../libs/core/data-access-firebase/src/index.ts"],
      "@atocha/core/ui": ["../../libs/core/ui/src/index.ts"],
      "@atocha/core/util": ["../../libs/core/util/src/index.ts"],
      "@atocha/menu-matriarch/ui": [
        "../../libs/menu-matriarch/ui/src/index.ts"
      ],
      "@atocha/menu-matriarch/types": [
        "../../libs/menu-matriarch/types/src/index.ts"
      ],
      "@env/*": ["src/environments/*"],
      "@services/*": ["src/app/core/services/*"],
      "@shared/*": ["src/app/shared/*"]
    },
    "target": "es2020",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}
