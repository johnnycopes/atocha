{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/types-globetrotter/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:swc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/types-globetrotter",
        "main": "libs/types-globetrotter/src/index.ts",
        "tsConfig": "libs/types-globetrotter/tsconfig.lib.json",
        "assets": ["libs/types-globetrotter/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/types-globetrotter/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/types-globetrotter"],
      "options": {
        "jestConfig": "libs/types-globetrotter/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
