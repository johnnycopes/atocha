{
  "name": "spirit-islander-ui",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/spirit-islander/ui/src",
  "prefix": "ui",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/spirit-islander/ui"],
      "options": {
        "jestConfig": "libs/spirit-islander/ui/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": [
          "libs/spirit-islander/ui/**/*.ts",
          "libs/spirit-islander/ui/**/*.html"
        ]
      }
    }
  },
  "tags": ["scope:spirit-islander", "type:ui"]
}
