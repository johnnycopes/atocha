{
  "name": "spirit-islander-feature-config",
  "projectType": "library",
  "sourceRoot": "libs/spirit-islander/feature-config/src",
  "prefix": "app",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/spirit-islander/feature-config"],
      "options": {
        "jestConfig": "libs/spirit-islander/feature-config/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/spirit-islander/feature-config/**/*.ts",
          "libs/spirit-islander/feature-config/**/*.html"
        ]
      }
    }
  },
  "tags": ["scope:spirit-islander", "type:feature"]
}
