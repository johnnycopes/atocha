{
  "name": "menu-matriarch-feature-planner",
  "projectType": "library",
  "sourceRoot": "libs/menu-matriarch/feature-planner/src",
  "prefix": "app",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/menu-matriarch/feature-planner"],
      "options": {
        "jestConfig": "libs/menu-matriarch/feature-planner/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/menu-matriarch/feature-planner/**/*.ts",
          "libs/menu-matriarch/feature-planner/**/*.html"
        ]
      }
    }
  },
  "tags": ["scope:menu-matriarch", "type:feature"]
}
