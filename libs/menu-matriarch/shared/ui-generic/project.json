        "jestConfig": "libs/menu-matriarch/shared/ui-generic/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/menu-matriarch/shared/ui-generic/**/*.ts",
          "libs/menu-matriarch/shared/ui-generic/**/*.html"
        ]
      }
    }
  },
  "tags": ["scope:menu-matriarch", "type:ui"]
}
