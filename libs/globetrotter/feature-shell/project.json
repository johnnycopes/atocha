{
  "name": "globetrotter-feature-shell",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/globetrotter/feature-shell/src",
  "prefix": "app",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/globetrotter/feature-shell"],
      "options": {
        "jestConfig": "libs/globetrotter/feature-shell/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": [
          "libs/globetrotter/feature-shell/**/*.ts",
          "libs/globetrotter/feature-shell/**/*.html"
        ]
      }
    }
  },
  "tags": ["scope:globetrotter", "type:feature"]
}
