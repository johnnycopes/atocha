{
  "projectType": "library",
  "sourceRoot": "libs/lorenzo/feature-shell/src",
  "prefix": "atocha",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/lorenzo/feature-shell"],
      "options": {
        "jestConfig": "libs/lorenzo/feature-shell/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/lorenzo/feature-shell/**/*.ts",
          "libs/lorenzo/feature-shell/**/*.html"
        ]
      }
    }
  },
  "tags": ["scope:lorenzo tag:feature"]
}
