{
  "name": "lorenzo-ui",
  "projectType": "library",
  "sourceRoot": "libs/lorenzo/ui/src",
  "prefix": "ui",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/lorenzo/ui"],
      "options": {
        "jestConfig": "libs/lorenzo/ui/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/lorenzo/ui/**/*.ts",
          "libs/lorenzo/ui/**/*.html"
        ]
      }
    }
  },
  "tags": ["scope:lorenzo", "type:ui"]
}
